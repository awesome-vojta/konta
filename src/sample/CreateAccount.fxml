<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CreateAccount">
    <children>
        <Button fx:id="backButton" layoutX="66.0" layoutY="348.0" mnemonicParsing="false" onAction="#goBack" text="Go Back" />
      <TextField fx:id="first_name" layoutX="36.0" layoutY="117.0" prefHeight="26.0" prefWidth="87.0" promptText="Jméno" />
      <TextField fx:id="second_name" layoutX="138.0" layoutY="117.0" prefHeight="26.0" prefWidth="87.0" promptText="Příjmení" />
      <TextField fx:id="rc" layoutX="36.0" layoutY="157.0" prefHeight="26.0" prefWidth="87.0" promptText="RČ" />
      <TextField fx:id="idNum" layoutX="140.0" layoutY="157.0" prefHeight="26.0" prefWidth="87.0" promptText="číslo OP" />
      <Button fx:id="saveButton" layoutX="36.0" layoutY="200.0" mnemonicParsing="false" onAction="#saveAccount" text="Uložit" />
      <CheckBox fx:id="checkBox" layoutX="247.0" layoutY="121.0" mnemonicParsing="false" text="Úvěr" />
      <TextField fx:id="debt" layoutX="239.0" layoutY="157.0" prefHeight="26.0" prefWidth="105.0" promptText="Úvěr, částka" />
      <TextField fx:id="percentage" layoutX="361.0" layoutY="157.0" prefHeight="26.0" prefWidth="78.0" promptText="RPSN %" />
    </children>

</AnchorPane>
